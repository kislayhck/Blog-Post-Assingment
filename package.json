{
  "name": "nodeapi",
  "version": "1.0.0",
  "description": "A Node Js API",
  "main": "index.js",
  "scripts": {
    "client-install": "npm install --prefix my-app",
    "start": "node app.js",
    "server": "nodemon app.js ",
    "client": "npm start --prefix my-app",
    "dev": "concurrently \"npm run server\" \"npm run client\""
