    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-validator": "^6.1.0",
    "formidable": "^1.2.1",
    "google-auth-library": "^4.2.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.6.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "uuid": "^3.3.2"
  }
